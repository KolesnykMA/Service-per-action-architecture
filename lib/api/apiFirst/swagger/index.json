{
  "openapi" : "3.0.0",
  "info" : {
    "description" : "Api for API mobile",
    "version" : "1.0.0",
    "title" : "Dada API mobile"
  },
  "servers" : [ {
    "url" : "http://localhost:5000/api/v1/first"
  }, {
    "url" : "http://3.11.111.111/api/v1/first"
  }],
  "tags" : [ {
    "name" : "auth"
  } ],
  "paths" : {
    "/auth/login/manual" : {
      "post" : {
        "tags" : [ "auth" ],
        "requestBody" : {
          "content" : {
            "application/json" : {
              "schema" : {
                "$ref" : "#/components/schemas/LoginManual"
              }
            }
          },
          "required" : true
        },
        "summary" : "Login user with email and password",
        "responses" : {
          "200" : {
            "description" : "",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/LoginSuccessfulResponse"
                }
              }
            }
          },
          "500" : {
            "description" : "",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    },
    "/auth/login/sns" : {
      "post" : {
        "tags" : [ "auth" ],
        "requestBody" : {
          "content" : {
            "application/json" : {
              "schema" : {
                "$ref" : "#/components/schemas/LoginSns"
              }
            }
          },
          "required" : true
        },
        "summary" : "Login user with sns token and sns token type (For inst, kakao, google etc)",
        "responses" : {
          "200" : {
            "description" : "",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/LoginSuccessfulResponse"
                }
              }
            }
          },
          "500" : {
            "description" : "",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    }
  },
  "components" : {
    "securitySchemes" : {
      "bearerAuth" : {
        "type" : "apiKey",
        "name" : "Authorization",
        "in" : "header"
      }
    },
    "schemas" : {
      "ErrorResponse" : {
        "type" : "object",
        "properties" : {
          "error" : {
            "type" : "object",
            "properties" : {
              "code" : {
                "type" : "string"
              },
              "fields" : {
                "type" : "object"
              },
              "message" : {
                "type" : "string"
              }
            }
          }
        }
      },
      "EmptySuccessfulResponse" : {
        "type" : "object",
        "properties" : {
          "data" : {
            "type" : "object"
          }
        }
      },


      "LoginManual" : {
        "type" : "object",
        "properties" : {
          "email" : {
            "type" : "string",
            "example" : "alice"
          },
          "password" : {
            "type" : "string",
            "example" : "123"
          }
        }
      },
      "LoginSns" : {
        "type" : "object",
        "properties" : {
          "snsToken" : {
            "type" : "string",
            "example" : "max"
          },
          "snsType" : {
            "type" : "string",
            "example" : "123456"
          }
        }
      },
      "LoginSuccessfulResponse" : {
        "type" : "object",
        "properties" : {
          "data" : {
            "type" : "object",
            "properties" : {
              "token" : {
                "type" : "string"
              }
            }
          }
        }
      }
    }
  }
}
